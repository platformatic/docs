{
  "name": "platformatic-oss-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "node loadEnv.js",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "sync": "echo $DOCS; node -e \"console.log(JSON.stringify(require('$DOCS/sidebars.js'), null, 2))\" > sidebars.json && cpx $DOCS'/**/*' docs",
    "sync-and-start": "npm run sync && concurrently \"cpx $DOCS'/**/*' docs -w --no-initial \" \"npm run start\"",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@astrojs/prism": "^1.0.2",
    "@docusaurus/core": "^3.5.1",
    "@docusaurus/plugin-client-redirects": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@docusaurus/types": "^3.5.1",
    "@mdx-js/react": "^3.1.1",
    "@orama/plugin-docusaurus-v3": "^3.1.14",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "execa": "^9.6.0",
    "npm": "^10.9.4",
    "posthog-docusaurus": "^2.0.4",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.2",
    "tailwindcss": "^3.4.18",
    "undici": "^6.22.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "cpx": "^1.5.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.6.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "snazzy": "^9.0.0",
    "standard": "^17.1.2",
    "webpack": "^5.102.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^22"
  },
  "standard": {
    "ignore": [
      "docs",
      "static",
      "versioned_docs",
      "versioned_sidebars"
    ]
  }
}
