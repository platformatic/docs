{
  "name": "platformatic-oss-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "node loadEnv.js",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "sync": "echo $DOCS; node -e \"console.log(JSON.stringify(require('$DOCS/sidebars.js'), null, 2))\" > sidebars.json && cpx $DOCS'/**/*' docs",
    "sync-and-start": "npm run sync && concurrently \"cpx $DOCS'/**/*' docs -w --no-initial \" \"npm run start\"",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@astrojs/prism": "^1.0.2",
    "@docusaurus/core": "^3.5.1",
    "@docusaurus/plugin-client-redirects": "^3.5.1",
    "@docusaurus/preset-classic": "^3.5.1",
    "@docusaurus/types": "^3.5.1",
    "@mdx-js/react": "^3.0.1",
    "@storyblok/react": "^3.0.10",
    "buffer": "^6.0.3",
    "clsx": "^1.2.1",
    "concurrently": "^9.0.1",
    "cpx2": "^5.0.0",
    "dotenv": "^16.4.5",
    "execa": "^9.3.0",
    "html-react-parser": "^5.1.12",
    "i": "^0.3.7",
    "install": "^0.13.0",
    "npm": "^10.8.3",
    "param-case": "^4.0.0",
    "posthog-docusaurus": "^2.0.1",
    "prism-react-renderer": "^2.3.1",
    "prismjs": "^1.29.0",
    "process": "^0.11.10",
    "proxy-your-https-macos": "^1.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-lottie-player": "^2.1.0",
    "semver": "^7.6.3",
    "storyblok-rich-text-react-renderer": "^2.9.2",
    "tailwindcss": "^3.4.10",
    "undici": "^6.19.7"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.4.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "snazzy": "^9.0.0",
    "standard": "^17.1.0",
    "webpack": "^5.93.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^22"
  },
  "standard": {
    "ignore": [
      "docs",
      "static",
      "versioned_docs",
      "versioned_sidebars"
    ]
  }
}
